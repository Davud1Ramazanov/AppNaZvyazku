<Page x:Class="TestZvyazok.Pages.Tariff"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TestZvyazok.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="2000"
      Title="Tariff" Background="White">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Тариф" FontSize="25" FontWeight="Bold" VerticalAlignment="Top"/>
        <WrapPanel Grid.Row="1">
        <local:UserControlTariff x:Name="mycontrol"></local:UserControlTariff>
        <local:UserControlTariff x:Name="mycontrol1"></local:UserControlTariff>
        <local:UserControlTariff x:Name="mycontrol2"></local:UserControlTariff>
        <local:UserControlTariff x:Name="mycontrol3"></local:UserControlTariff>
        <local:UserControlTariff x:Name="mycontrol4"></local:UserControlTariff>
        </WrapPanel>
    </Grid>
</Page>
